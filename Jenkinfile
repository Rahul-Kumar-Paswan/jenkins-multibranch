
def gv

pipeline{
    agent any

    stages {
        stage("init") {
            steps {
                script {
                    gv = load "script.groovy"
                }
            }
        }
        stage('Build') {
            steps{
                script{
                    gv.buildApp()
                }
            }
        }
        stage("Test"){
            steps{
                script{
                    gv.testApp()
                }
            }
        }
        stage("Deploy"){
            steps{
                script{
                    gv.deployApp()
                }
            }
        }
        stage("Prod"){
            steps{
                script{
                    gv.prodApp()
                }
            }
        }
    }
    post{
        always{
            echo " testing feature branch always "
        }

    }
    
}